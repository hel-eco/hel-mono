{
  "name": "@hel-demo/mono-libs",
  "version": "0.0.1",
  "description": "hel-mono demo libs",
  "repository": {
    "type": "git",
    "url": "https://github.com/hel-eco/hel-mono",
    "directory": "packages/mono-libs"
  },
  "license": "MIT",
  "author": {
    "name": "fancyzhong"
  },
  "main": "./src/index",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./hel": {
      "types": "./src/.hel/entrance/libTypes.ts",
      "import": "./src/.hel/entrance/libTypes.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "./hel": {
        "types": "./src/.hel/entrance/libTypes.ts",
        "import": "./src/.hel/entrance/libTypes.ts"
      }
    }
  },
  "files": [
    "dist",
    "hel_dist",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "start": "node ../../dev/scripts/start.js",
    "start:hel": "node ../../dev/scripts/hel/start.js",
    "start:hwl": "node ../../dev/scripts/hel/startWL.js",
    "start:hwr": "node ../../dev/scripts/hel/startWR.js",
    "hint": "node ../../dev/scripts/hel/hint.js",
    "build": "pnpm run build:npm",
    "build:nbs": "pnpm run build:npm && pnpm run build:helbs",
    "build:local": "cross-env-shell \"pnpm run build:helbs && pnpm run build:meta\"",
    "build:ci": "node ../../dev/scripts/hel/buildCI",
    "build:hel": "node ../../dev/scripts/hel/build.js",
    "build:hels": "node ../../dev/scripts/hel/buildS",
    "build:helbs": "node ../../dev/scripts/hel/buildBS",
    "build:meta": "node ../../dev/scripts/hel/meta",
    "build:npm": "tsup",
    "tsc": "tsc --project ./tsconfig.build.json && tsc-alias -p ./tsconfig.json ",
    "start:test": "npm run test",
    "start:test:once": "npm run test:once",
    "test": "node ../../dev/scripts/test --config ../../dev/config/jest/jest.config.js",
    "test:once": "cross-env ONCE=1 node ../../dev/scripts/test --config ../../dev/config/jest/jest.config.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "babel-cli": "^6.26.0"
  },
  "deprecated": false,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "18.12.1",
    "pnpm": "8.6.8"
  }
}